{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
        "source": "/notification/**",
        "function": "notificationPage"
    },
    {
        "source": "/sendNotification**",
        "function": "sendNotification"
    }]
  }
}
